			<key>end</key>
			<string>^-$</string>
			<key>name</key>
			<string>comment.git-savvy.status.key-bindings-menu</string>
			<key>patterns</key>
			<array>
				<dict>
					<key>captures</key>
					<dict>
						<key>1</key>
