					<string>meta.git-savvy.branches.branch</string>
				</dict>
				<dict>
					<key>match</key>
					<string>^  â–¸ (.+)\n$</string>
					<key>name</key>
					<string>string.other.git-savvy.branches.active-branch</string>
				</dict>
			</array>
		</dict>
		<dict>
			<key>comment</key>
			<string>ux note</string>
