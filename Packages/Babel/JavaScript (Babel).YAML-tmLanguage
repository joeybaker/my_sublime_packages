        '5': {name: entity.name.function.js}
        '6': {name: keyword.operator.assignment.js}
        '7': {name: storage.type.js}
        '8': {name: storage.type.function.js}
        '9': {name: keyword.generator.asterisk.js}
        '10': {name: entity.name.function.js}
      end: (?<=\))
      patterns:
